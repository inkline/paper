{
  "name": "universal",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "rimraf lib && vite build && npm run build:mjs && npm run build:cjs && ts-node scripts/build.ts",
    "build:mjs": "tsc -d -p tsconfig.mjs.json && npm run build:mjs:transform",
    "build:mjs:transform": "mjscjs -p tsconfig.mjs.json",
    "build:cjs": "tsc -d -p tsconfig.cjs.json && npm run build:cjs:transform",
    "build:cjs:transform": "mjscjs -p tsconfig.cjs.json",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.js",
    "test": "vitest",
    "prerelease": "npm run test && npm run build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.3",
    "@testing-library/vue": "6.4.2",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "@vitejs/plugin-vue": "2.2.0",
    "eslint": "8.10.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "jsdom": "^19.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "shelljs": "0.8.5",
    "ts-node": "10.6.0",
    "tsconfig-paths": "3.13.0",
    "typescript": "4.5.4",
    "vite": "2.8.0",
    "vitest": "0.5.8",
    "vue": "3.2.25",
    "vue-tsc": "0.29.8"
  }
}
